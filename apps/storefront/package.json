{
  "name": "storefront",
  "type": "module",
  "private": true,
  "version": "0.0.0",
  "prettier": "@bigcommerce/eslint-config/prettier",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:staging": "tsc && vite build --mode staging",
    "build:tier1": "tsc && vite build --mode tier1",
    "lint": "yarn lint:dependencies && yarn lint:eslint",
    "lint:eslint": "eslint . --ext .ts,.tsx,.js --cache --cache-location ./node_modules/.cache/eslint --max-warnings 0 .",
    "lint:dependencies": "depcruise --config .dependency-cruiser.cjs . --output-type err-long",
    "format": "yarn lint --fix",
    "preview": "vite preview",
    "test": "vitest",
    "coverage": "vitest --coverage",
    "generate": "graphql-codegen",
    "generate:local": "CODEGEN_ENV=local graphql-codegen",
    "start": "http-server ./dist --cors"
  },
  "dependencies": {
    "@b3/global-b3": "*",
    "@b3/hooks": "*",
    "@b3/lang": "*",
    "@b3/utils": "*",
    "@emotion/cache": "^11.9.3",
    "@emotion/react": "^11.9.3",
    "@emotion/styled": "^11.9.3",
    "@mui/icons-material": "^5.8.4",
    "@mui/material": "^5.8.7",
    "@mui/styles": "^5.8.7",
    "@mui/x-date-pickers": "5.0.20",
    "@reduxjs/toolkit": "^2.2.1",
    "@types/babel__core": "^7.1.19",
    "@types/lodash-es": "^4.17.8",
    "@types/pdfobject": "^2.2.3",
    "@types/react-resizable": "^3.0.4",
    "copy-to-clipboard": "^3.3.3",
    "date-fns": "^2.28.0",
    "dayjs": "^1.11.10",
    "graphql": "^16.8.1",
    "js-cookie": "^3.0.5",
    "lodash-es": "^4.17.21",
    "pdfobject": "^2.2.12",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "react-hook-form": "^7.33.1",
    "react-infinite-scroller": "^1.2.6",
    "react-mui-dropzone": "^4.0.6",
    "react-pdf": "^7.1.2",
    "react-redux": "^9.1.0",
    "react-resizable": "^3.0.5",
    "react-router-dom": "6",
    "redux-persist": "^6.0.0",
    "ts-tracking-number": "^1.0.16",
    "uuid": "^9.0.0",
    "zod": "^3.24.4"
  },
  "devDependencies": {
    "@b3/tsconfig": "*",
    "@faker-js/faker": "^9.7.0",
    "@graphql-codegen/cli": "^5.0.2",
    "@graphql-codegen/client-preset": "^4.2.5",
    "@testing-library/dom": "^10.1.0",
    "@testing-library/jest-dom": "^6.4.6",
    "@testing-library/react": "^16.0.0",
    "@testing-library/user-event": "^14.2.5",
    "@types/crypto-js": "^4.2.1",
    "@types/js-cookie": "^3.0.6",
    "@types/jsdom": "^21.1.7",
    "@types/lodash": "^4.14.190",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.0.0",
    "@types/react-infinite-scroller": "^1.2.3",
    "@types/uuid": "^8.3.4",
    "@vitejs/plugin-legacy": "^5.4.1",
    "@vitejs/plugin-react": "^4.3.1",
    "@vitest/coverage-istanbul": "^1.4.0",
    "babel-plugin-import": "^1.13.8",
    "c8": "^7.11.3",
    "dependency-cruiser": "^16.3.3",
    "eslint": "^7.32.0 || ^8.2.0",
    "eslint-config-b2b": "*",
    "jsdom": "^24.1.0",
    "msw": "^2.8.4",
    "rollup-plugin-visualizer": "^5.12.0",
    "terser": "^5.14.2",
    "typescript": "^5.8.3",
    "vite": "^5.4.19",
    "vitest": "^1.6.1"
  }
}
